{
  "manifest_version": 3,
  "name": "LinkedIn Extension",
  "version": "1.0",
  "description": "LinkedIn Extension for Automated Actions",
  "permissions": ["activeTab"],
  "action": {
    "default_icon": "noBS_AR.png"
  },
  "icons": {
    "16": "noBS_AR.png",
    "48": "noBS_AR.png",
    "128": "noBS_AR.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
